        function loadSchedule(pPatientID,pClinicID,pDate){

        }

        function launchSchedule(pPatientID,pClinicID,pDate,pLoadOnly) {
            var lURL = "/Controls/Pages/Secure/Business/Schedule.aspx?load=true";
            if(pPatientID != null)
            {
                lURL += "&pid=" + pPatientID;
            }
            if (pClinicID != null) {
                lURL += "&cid=" + pClinicID;
            }
            if (pDate != null) {
                lURL += "&dtm=" + pDate;
            }

            $('#frmSchedule').attr('src', lURL);

            if(!pLoadOnly){
                $("#dialogSchedule").dialog("option", "title", "Schedule");
                $("#dialogSchedule").dialog("open");
            }
        }

        function showSchedule(pLoadOnly)
        {
            var lPatientID = $.getUrlVar('id');
            if(isPatientAdmin() && lPatientID != null)
            {
                launchSchedule(lPatientID, null, null, pLoadOnly);
            }
            else
            {
                launchSchedule(null, null, null, pLoadOnly);
            }
        }

        function isPatientAdmin()
        {
            var lIsPatientAdmin = false;

            var lCurrentURL = window.location.href;
            if(lCurrentURL.indexOf("page=Patients/Patient") > 0 || lCurrentURL.indexOf("page=Patients%2fPatient") > 0)
            {
                lIsPatientAdmin = true;
            }

            return lIsPatientAdmin;
        }

        function closeSchedule() {
            if(isPatientAdmin())
            {
                var lPatientID = $.getUrlVar('id');
                if(lPatientID != null)
                {
                    $.cookie("PatientAdminTab", "3", { expires: 999999999, path: '/' });
                    window.location = "/Controls/Pages/Secure/Index.aspx?page=Patients/Patient&id=" + lPatientID + "&action=e";
                }
            }
        }

