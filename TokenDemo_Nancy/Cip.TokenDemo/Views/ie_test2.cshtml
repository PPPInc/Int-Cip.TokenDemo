                            <script languague="javascript">
                                function EndRequestHandlerLogout(sender, args) {
                                    if (args.get_error() == undefined) {
                                        initializeJqueryElementsLogout();
                                    }
                                }

                                var lIsForgotPassword = false;
                                initializeJqueryElementsLogout = function(){
                                    $("#dialogLogin").dialog({
                                        modal: true,
                                        minWidth: 530,
                                        minHeight: 200,
                                        draggable: false,
                                        autoOpen: false
                                    });

                                    $("#lnkLogin").click(function(){
                                        lIsForgotPassword = false;
                                        $("#dvForgotPassword").hide();
                                        $("#dvLoginForm").show();

                                        location.hash = '';
                                        $("#dialogLogin").dialog("option","title","CounselEAR Login");
                                        $("#dialogLogin").dialog("open");

                                        if($("#header1_logout_txtEmailAddress").val() == "")
                                        {
                                            $('#header1_logout_txtEmailAddress').focus();
                                        }
                                        else
                                        {
                                            $('#header1_logout_txtPassword').focus();
                                        }
                                    });

                                    $("#lnkForgotPassword").click(function(){
                                        lIsForgotPassword = true;
                                        $("#dvForgotPassword").show();
                                        $("#dvLoginForm").hide();
                                    });

                                    $("#header1_logout_btnCancel").click(function(){
                                        $("#dialogLogin").dialog("close");
                                    });

                                    $("#header1_logout_btnSubmit").button();
                                    $("#header1_logout_btnCancel").button();

                                    if(lIsForgotPassword)
                                    {
                                        $("#dvForgotPassword").show();
                                        $("#dvLoginForm").hide();
                                    }
                                }

                                $(document).ready(function () {
                                    initializeJqueryElementsLogout();

                                    $('#dialogLogin').parent().appendTo("form");

                                    Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandlerLogout);

                                    var lReturnURL = $.getUrlVar('ReturnUrl');
                                    if (lReturnURL != null) {
                                        $("#lnkLogin").click();
                                    }
                                });

                                $(window).load(function () {
                                    $('#dvLogin').show();
                                });
                            </script>
